#include <stdio.h>

int main() {
    int num;
    int count =10;
    printf("Enter the num:");
    scanf("%d",&num);
    while (count >= 1){
        int product=num*count;
        printf("%d*%d=%d\n",num,count,product);
        count-=1;
    }
   

    return 0;
}
